{
  "name": "makers-tech-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test:checkout": "node -r ts-node/register src/scripts/testCheckoutFlow.ts",
    "seed:products": "ts-node-dev --transpile-only src/scripts/seedProducts.ts",
    "seed:roles": "ts-node src/scripts/seedRoles.ts",
    "seed:admin": "ts-node src/scripts/createAdmin.ts",
    "db:init": "ts-node src/scripts/initializeAll.ts",
    "db:categories": "ts-node src/scripts/initializeCategories.ts",
    "db:orders": "ts-node src/scripts/initializeOrders.ts",
    "db:offers": "ts-node src/scripts/initializeOffers.ts"
  ,
    "ip:egress": "node scripts/print-egress-ip.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.1",
    "multer": "^2.0.2",
    "react-router-dom": "^6.30.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "mongodb-memory-server": "^10.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
